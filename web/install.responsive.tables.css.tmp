.widget-container ul.recentcomments .avatar{margin-right:5px}
.rc_cmt_author{display:inline-block}
.rc_cmt_date{display:inline-block;margin-left:1em}
.cat_cmt{margin-left:1em}
#pagewrap{width:1140px;max-width:98%;margin:0 auto;position:relative;min-height:100%}
#commentsx{color:#fff;font-size:1em;margin-top:.7em}
#comments,.nocomments{padding:0 5.5% .8em;font-size:1.15em}
.nocomments{display:none}
#trackback{padding:0 5.5% 1em;color:#999}
