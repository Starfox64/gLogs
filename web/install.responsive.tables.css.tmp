#commentform textarea{width:90%}
}
@media screen and (max-width: 480px) {
.relatedposts li{min-height:42px}
}
@media screen and (max-width: 320px) {
#topnav{float:none;text-align:center;max-width:300px;min-width:100px}
#topnav ul li{display:block}
.page_navi::after{position:relative}
#header .search{float:none;margin-bottom:15px}
}
.clear{clear:both;float:none}
.alignleft{float:left}
.alignright{float:right}
.aligncenter{display:block;margin-left:auto;margin-right:auto}
