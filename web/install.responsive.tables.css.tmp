body{margin:0;padding:0;border:none;background:url(http://i.imgur.com/jIqjwRZ.jpg) no-repeat;background-attachment:fixed;background-size:cover;font-family:Lucida Sans Unicode,'Microsoft Yahei',Trebuchet MS,Arial,Helvetica,sans-serif}
img,/*box*/
div,p,blockquote,/* heading */
h1,h2,h3,h4,h5,h6,/*list*/
ul,ol,li,dl,dt,dd,/*form*/
form,fieldset,textarea,/*address*/
address,/*strong*/
strong,/*block*/
article,aside,dialog,figure,footer,header,hgroup,nav,section{margin:0;padding:0;border:0;font-size:100%;font-weight:400;font-style:normal;text-decoration:none;list-style-type:none}
.design ul li a,.wordpress ul li a{color:#777;font-size:.875em}
h1{font-size:1.6em}
h2{font-size:1.5em}
h3{font-size:1.4em}
h4{font-size:1.3em}
h5{font-size:1.2em}
h6{font-size:1em}
article,aside,dialog,figure,footer,header,hgroup,nav,section{display:block}
p{padding:.5em 0;word-wrap:break-word}
blockquote{margin:20px 0 20px 15px;padding:8px 10px 5px;border:1px solid #E1E1E1;color:#444;background:#FAFAFA}
blockquote p{margin-top:5px;margin-bottom:5px}
pre{padding:15px 0;margin:10px 0;overflow:auto;width:90%}
code{color:#100;background:#f9f9f9;border:1px solid silver;margin:0 0 1.5em;padding:2px 3px;overflow:auto}
table{margin:1em 0;border:1px solid #ccc;background-color:#fff}
table th{background-color:#eee;padding:5px 10px}
table td{border-right:1px solid #eee;border-bottom:1px solid #eee;padding:2px 8px}
em{font-weight:700;font-style:italic}
strong{font-size:1em;font-weight:700}
dl{margin-bottom:1em}
dt{font-weight:700}
dd{margin-bottom:1em}
ul,ol{list-style:none;margin:0 0 1em 1em}
ul{list-style:disc}
ol{list-style:decimal}
li > ul,li > ol{margin:0 0 0 20px}
th{text-align:left;font-weight:400}
a,a:focus,a:active{outline:none;color:#307BD1;text-decoration:none;color:#307BD1}
a:hover{color:#183B85;border-color:#41b7d8}
a:link{-webkit-tap-highlight-color:#e53b2c}
img{vertical-align:top;max-width:100%;height:auto}
object,framework,embed{max-width:100%}
#header{margin:0;width:100%;height:50px;z-index:9;background:rgba(255,255,255,0.3);-webkit-box-shadow:0 4px 0 0 rgba(0,0,0,0.1);box-shadow:0 4px 0 0 rgba(0,0,0,0.1)}
