#title a{border:none;padding-left:60px;position:relative;top:4px;font-size:1.4em;color:#ddd;line-height:2em}
#header ul li a{float:left}
.image-fixed{width:60px;position:fixed;bottom:10px;right:20px;z-index:0}
.minisidebar .widget-container{margin-right:1em}
.minisidebar li a{font-size:1em;color:#3498db}
