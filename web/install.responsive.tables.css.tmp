.container{margin-right:350px}
#content{width:100%;float:left;margin-right:-350px;max-width:1800px}
.minisidebar{width:280px;max-width:23%;float:right;padding:2em 0 2em 15px;margin:65px 10px 0 20px;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;background:rgba(15,2,0,0.95);border:1px solid rgba(41,128,185,1);box-shadow:0 0 10px rgba(41,128,185,1);text-align:center}
#main{width:70%;float:right;padding:4em 0 2em}
