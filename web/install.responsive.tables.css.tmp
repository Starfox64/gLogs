.related-title,.relatedposts{padding:0 5.5% 1em;font-size:.875em;font-weight:700}
.related-title a{display:inline-block;margin:-3px 5px 3px 0;padding:4px 8px;font-size:.75em;background:#e7e7e7;text-decoration:none;text-shadow:0 1px rgba(255,255,255,0.7);float:left;position:relative;color:#777}
.related-title a:active{box-shadow:inset 0 -2px 1px rgba(255,255,255,0.2),inset 0 3px 2px rgba(0,0,0,0.12)}
.relatedposts li{padding-bottom:.25em;margin-top:.25em;border-bottom:1px solid #eee;font-size:1em;color:#777;clear:right;min-height:21px}
