#breadcrumb .more-cat{position:absolute;background:#fff;padding:.75em 1em 1em;line-height:1.7em;box-shadow:2px 2px 3px #ccc;left:-1px;top:16px;display:none;border-left:1px solid #ddd;border-top:1px solid #ddd;min-width:100px;color:#aaa}
#breadcrumb .more-cat ul{margin-left:0}
