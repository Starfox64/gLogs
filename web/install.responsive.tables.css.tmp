#content,.container{margin-right:0;display:block}
#header .search{float:none;max-width:100%;position:relative;margin:-54px auto 15px}
.search-toggle:after{height:54px;line-height:55px}
#header .search-inner{margin-top:14px;margin-right:15px}
#header .search form input{background:#4597D6}
#url,#email,#author{margin-right:1em;width:62%}
#commentform textarea{width:80%}
#footer{padding-left:5.5%}
}
@media screen and (max-width: 600px) {
#breadcrumb,.section.single .info .bdlikebutton-medium-blue{display:none}
.mini-select{display:block;padding:0 5.5% 2em}
.mini-select #subsection-select{width:100%}
.children{margin-left:0}
.section.single .postinfo{background-image:none;padding-left:10px}
