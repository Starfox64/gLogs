.relatedposts li .cmt_num{float:right}
.flip,.tags{padding:1em 5.5%;clear:both;display:block;overflow:hidden;display:none}
.tags{font-size:.85em;margin-left:11px}
.tags a{background:#929191;padding:3px 5px;position:relative;line-height:35px;margin-right:15px;border:none;color:#fff}
.tags a:before{content:'';border-color:transparent #929191 transparent transparent;border-width:12px;display:block;position:absolute;left:-24px;border-style:solid;top:0}
.tags a:after{content:'';border-radius:10px;border-width:3px;display:block;position:absolute;left:-3px;border-style:solid;top:8px;border-color:#fff}
.prevpost:before{font-family:fontawesome;content:'\f104';margin-right:10px}
.nextpost:after{font-family:fontawesome;content:'\f105';margin-left:10px}
.nextpost{float:right;text-align:right}
.prevpost a,.nextpost a{color:#7f7f7f;padding:0;word-wrap:break-word;font-size:.875em}
.page_navi .pages,.page_navi .page-numbers{display:inline-block;padding:5px 10px;font-size:.75em;background:#e7e7e7;border:none;float:left;position:relative;color:#777}
.page_navi .current{background:#444;color:#eee}
.page_navi{margin-left:5.5%;position:relative;display:block;margin-top:2%;float:left;z-index:0}
#sidebar li a{color:#3498db}
#sidebar li a:hover{color:#2d42e4}
.widgettitle{padding:10px 0 6px;color:#fff;display:block}
.widget-container{margin-bottom:1em}
.widget-container ul{padding:0 5px 10px;font-size:.8125em;line-height:1.125em;border-top:1px solid #eee;margin-left:0}
.widget-container ul li{padding:.35em 0;color:#777;line-height:1.45em}
.widget-container ul li a{border-bottom:0}
